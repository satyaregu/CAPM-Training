{
  "xsappname": "capmapp",
  "scopes": [
    {
      "name": "$XSAPPNAME.Viewer",
      "description": "Able to view data from services"
    },
    {
      "name": "$XSAPPNAME.Admin",
      "description": "Able to edit data"
    }
  ],
  "attributes": [
    {
      "name": "BankName",
      "description": "BankName to allow row level security",
      "valueType": "string"
    }
  ],
  "role-templates": [
    {
      "name": "Viewer",
      "description": "View all the employess",
      "default-role-name": "Viewer: Authorized to Read All employess",
      "scope-references": [
        "$XSAPPNAME.Viewer"
      ],
      "attribute-references": [
        {
          "name": "BankName"
        }
      ]
    },
    {
      "name": "Admin",
      "description": "Edit the employee details",
      "scope-references": [
        "$XSAPPNAME.Admin"
      ]
    }
  ],
  "role-collections": [
    {
      "name": "UserViewerRC",
      "description": "User Viewer Role Collection",
      "role-template-references": [
        "$XSAPPNAME.Viewer"
      ]
    }
  ],
  "authorities": [
    "$ACCEPT_GRANTED_AUTHORITIES"
  ],
  "oauth2-configuration": {
    "token-validity": 1800,
    "redirect-uris": [
      "https://*.api.cf.us10-001.hana.ondemand.com/login/callback"
    ],
    "autoapprove": "false"
  },
  "xsenableasyncservice": "true"
}